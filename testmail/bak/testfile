open 'mail.sohu.com'


# open browser
open http://mail.sohu.com

# sleep 1 seconds
sleep 1
# input & textarea  with id or name
fill name username user101@sohu.com
fill id   password 111111

# button or link
click id update
click name new


# checkbox
check name enable
check id enable

# radio
set name aaa
set id bbb

# select
select name month
select id year

# clear the content or uncheck
clear name enable
clear id bbb

# show title
title

# show statusbar
status

# include content
include this will show in page or not

